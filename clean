
 ldi r16, 0xff
 out ddrb, r16
 out ddrd, r16
 ldi r17, 0
 out ddrc, r17

 ldi r16, 0xff
 out portd, r16
 ldi r16, 0b1000_0011
  out portb, r16
  ldi r16, 0
  out portb, r16

 
